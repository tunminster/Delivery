{
  "AppSettings": {
    "Secret": "TopSecret"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    },
    "Clients": {
      "Delivery.Api": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*",
  "AzureStorageConfig": {
    "ImageContainer": "images",
    "ThumbnailContainer": "thumbnails"
  },
  "ConnectionStings": {
    "DefaultConnection": "Database=DeliveryApp;"
  },
  "JwtIssuerOptions": {
    "Issuer": "webApi",
    "Audience": "http://localhost:5000/"
  }
}
